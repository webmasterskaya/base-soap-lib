{
  "name": "webmasterskaya/base-soap-lib",
  "description": "Набор базовых классов для построения PHP-SOAP приложения, совместимый с PHP >=8.2",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Webmasterskaya\\Soap\\Base\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Artem Vasilev",
      "email": "dev@webmasterskaya.xyz"
    }
  ],
  "require": {
    "php": "~8.2.0 || ~8.3.0 || ~8.4.0",
    "azjezz/psl": "^3.0",
    "php-soap/cached-engine": "~0.3",
    "php-soap/engine": "^2.14.0",
    "php-soap/encoding": "~0.15",
    "php-soap/psr18-transport": "^1.7",
    "php-soap/wsdl-reader": "~0.21",
    "psr/event-dispatcher": "^1.0"
  },
  "require-dev": {
    "nyholm/psr7": "^1.8",
    "friendsofphp/php-cs-fixer": "^3.65",
    "symfony/cache": "^6.4 || ^7.0",
    "symfony/http-client": "^7.3"
  },
  "scripts": {
    "cs": [
      "php-cs-fixer fix --ansi --dry-run --diff"
    ],
    "cs-fix": [
      "php-cs-fixer fix --ansi"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
