{
	"name": "webmasterskaya/base-soap-lib",
	"description": "Набор базовых классов для построения PHP-SOAP приложения, совместимый с PHP 7.1",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Webmasterskaya\\Soap\\Base\\": "src/"
		}
	},
	"authors": [
		{
			"name": "Artem Vasilev",
			"email": "dev@webmasterskaya.xyz"
		}
	],
	"require": {
		"php": "~8.2.0 || ~8.3.0 || ~8.4.0",
		"azjezz/psl": "^3.1",
		"php-soap/engine": "^2.13",
		"php-soap/ext-soap-engine": "^1.7",
		"php-soap/wsdl": "^1.12",
		"symfony/options-resolver": "^5.4"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.65",
		"vimeo/psalm": "^5.26"
	},
	"scripts" : {
		"cs": [
			"php-cs-fixer fix --ansi --dry-run --diff"
		],
		"cs-fix": [
			"php-cs-fixer fix --ansi"
		],
		"psalm" : [
			"psalm"
		]
	}
}
